{
  "name": "haakco/laravel-location-manager",
  "description": ":package_description",
  "license": "proprietary",
  "keywords": [
    "Laravel",
    "LocationManager"
  ],
  "authors": [
    {
      "name": "Tim Haak",
      "email": "tim@haak.co",
      "homepage": "https://haak.co"
    }
  ],
  "homepage": "https://github.com/haakco/laravel-location-manager",
  "require": {
    "haakco/laravel-postgres-helper": "@dev",
    "illuminate/support": "~7 || ~8 || ~9 || ~10",
    "pragmarx/countries": ">=0.7"
  },
  "require-dev": {
    "brainmaestro/composer-git-hooks": ">=2.8",
    "ergebnis/composer-normalize": ">=2.15",
    "orchestra/testbench": "~5 || ~6 || ~7 || ~8 || ~9 || ~10",
    "phpunit/phpunit": ">=9.5",
    "roave/security-advisories": "dev-latest",
    "symplify/easy-coding-standard": ">=9.4"
  },
  "repositories": {
    "haakco/laravel-postgres-helper": {
      "type": "vcs",
      "url": "git@github.com:haakco/laravel-postgres-helper.git"
    }
  },
  "autoload": {
    "psr-4": {
      "HaakCo\\LocationManager\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "HaakCo\\LocationManager\\Tests\\": "tests"
    }
  },
  "config": {
    "allow-plugins": {
      "ergebnis/composer-normalize": true
    }
  },
  "extra": {
    "laravel": {
      "aliases": {
        "LocationManager": "HaakCo\\LocationManager\\Facades\\LocationManager"
      },
      "providers": [
        "HaakCo\\LocationManager\\LocationManagerServiceProvider"
      ]
    }
  },
  "scripts": {
    "post-install-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postInstall",
      "vendor/bin/cghooks add --ignore-lock"
    ],
    "post-update-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postUpdate",
      "vendor/bin/cghooks update",
      "composer normalize"
    ],
    "cghooks": "vendor/bin/cghooks",
    "fix-code-format": [
      "./vendor/bin/phpcbf || true"
    ],
    "phpcs": [
      "@php vendor/bin/ecs check --no-interaction",
      "@php vendor/bin/ecs check --fix --no-interaction"
    ],
    "phpcs-check": [
      "@php vendor/bin/ecs check --no-interaction"
    ],
    "phpcs-no-cache": [
      "@php vendor/bin/ecs check --clear-cache --no-interaction",
      "@php vendor/bin/ecs check --no-interaction",
      "@php vendor/bin/ecs check --fix --no-interaction"
    ],
    "phpstan": "@php vendor/bin/phpstan analyse --no-interaction --memory-limit 1G",
    "psalm": "@php vendor/bin/psalm --show-info=false",
    "style-check": [
      "./vendor/bin/phpcs"
    ]
  }
}
